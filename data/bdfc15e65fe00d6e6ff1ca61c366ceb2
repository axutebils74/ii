aplayer.innerHTML = 0 + "/" + arr.length*2;
for(var i = 0; i < arr.length;i++){
    const j = i;
    const xhr =new XMLHttpRequest
    xhr.responseType = "blob";
    xhr.open("GET",arr[i]);
    xhr.onload = function(){
        arr[j] = URL.createObjectURL(xhr.response);
        if(!loaded) aplayer.innerHTML = cnt + '/' + arr.length*2
        if(++cnt == arr.length){
            loadMusic()
        }
    }
    xhr.send()
}
function loadMusic(){
    for(var i = 0; i < brr.length;i++){
        const j = i;
        const xhr =new XMLHttpRequest
        xhr.responseType = "blob";
        xhr.open("GET",brr[i]);
        xhr.onload = function(){
            brr[j] = URL.createObjectURL(xhr.response);
            if(!loaded) aplayer.innerHTML = cnt + '/' + brr.length*2
            if(++cnt == brr.length * 2){
                load()
            }
        }
        xhr.onerror = function(){
            if(++cnt == brr.length * 2){
                load()
            }
        }
        xhr.send()
    }
    setTimeout(load,16000);
}
function load(){
    if(loaded) return;
    loaded = 1;
    if(navigator.userAgent.indexOf("Mobile")!=-1){
        document.body.style.zoom = "1.2"
        zoom = 1.2
    }else{
        document.body.style.zoom = "2"
        zoom = 2
    }
    const ap = new APlayer({
    container: document.getElementById('aplayer'),
